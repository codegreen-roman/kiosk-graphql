{
  "name": "bora-sales-next",
  "author": "Smartbics",
  "license": "MIT",
  "version": "1.1.0",
  "scripts": {
    "dev": "next dev -p 3033",
    "dev-kihnu-kiosk": "cross-env NEXT_PUBLIC_THEME=kihnuKiosk yarn dev",
    "dev-kihnu-sales": "cross-env NEXT_PUBLIC_THEME=kihnuSales yarn dev",
    "build": "next build",
    "start": "next start",
    "next-lint": "next lint",
    "export": "next export",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest --config ./jest.config.js",
    "test-all": "yarn next-lint && yarn type-check && yarn test",
    "precopy-translations-en": "LANG=en node i18n/makeTranslationFiles.js",
    "copy-translations-en": "yes | cp i18n/source/en/generated/* i18n/locales/en/",
    "precopy-translations-et": "LANG=et node i18n/makeTranslationFiles.js",
    "copy-translations-et": "yes | cp i18n/source/et/generated/* i18n/locales/et/",
    "precopy-translations-ru": "LANG=ru node i18n/makeTranslationFiles.js",
    "copy-translations-ru": "yes | cp i18n/source/ru/generated/* i18n/locales/ru/",
    "precopy-translations-fi": "LANG=fi node i18n/makeTranslationFiles.js",
    "copy-translations-fi": "yes | cp i18n/source/fi/generated/* i18n/locales/fi/",
    "precopy-translations-de": "LANG=de node i18n/makeTranslationFiles.js",
    "copy-translations-de": "yes | cp i18n/source/de/generated/* i18n/locales/de/",
    "precopy-translations-sv": "LANG=sv node i18n/makeTranslationFiles.js",
    "copy-translations-sv": "yes | cp i18n/source/sv/generated/* i18n/locales/sv/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run next-lint"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint --quiet",
      "yarn format --quiet"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@apollo/react-ssr": "^4.0.0",
    "@date-io/date-fns": "^1.3.13",
    "@lifeomic/axios-fetch": "^1.4.2",
    "@material-ui/core": "^4.11.0",
    "@material-ui/pickers": "^3.2.10",
    "@types/ramda": "^0.27.14",
    "@types/styled-components": "^5.1.2",
    "@urql/exchange-retry": "^0.2.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-server-micro": "^2.17.0",
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.2.6",
    "graphql-tag": "^2.11.0",
    "graphql-ws": "^1.11.0",
    "i18next": "^19.8.3",
    "micro-cors": "^0.1.1",
    "nanoid": "^3.1.20",
    "next": "^12.1.0",
    "next-urql": "^2.1.1",
    "nextjs-fonts": "^0.22.0",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-contenteditable": "^3.3.5",
    "react-countdown": "^2.3.0",
    "react-dom": "^16.13.1",
    "react-hotkeys-hook": "^3.2.1",
    "react-i18next": "^11.7.3",
    "react-is": "^16.13.1",
    "react-simple-keyboard": "^2.5.36",
    "styled-components": "^5.1.1",
    "subscriptions-transport-ws": "^0.9.18",
    "urql": "^1.11.5",
    "winston": "^3.3.3",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.10",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/testing-library__react": "^10.0.0",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^25.2.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-graphql-tag": "^3.0.0",
    "babel-plugin-jsx-display-if": "^3.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "8",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "jest": "^25.2.3",
    "jest-enzyme": "^7.1.2",
    "jest-styled-components": "^7.0.3",
    "jest-transform-graphql": "^2.1.0",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "prettier": "^2.5.1",
    "react-svg-loader": "^3.0.3",
    "svgo-loader": "^2.2.1",
    "typescript": "4"
  },
  "engines": {
    "node": ">= 14"
  }
}
